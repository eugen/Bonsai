loadAssembly "System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"
import .Color .System.Drawing.Color

defun .rgbHandler .r .g .b {
	Color .FromArgb r g b
}

dataHandlers .Add .rgb rgbHandler

= .basicColors [#
  .white [rgb 255 255 255 ]
  .black [rgb 0 0 0 ]
  .red [rgb 255 0 0 ]
  .green [rgb 0 255 0 ]
  .blue [rgb 0 0 255 ]
]




= .user (object .clone .UserProto)
user .method .fullName { (self .firstName) .+ " " (self .lastName) }
= .john (user .clone .JohnObject)
john .= .firstName "John"
john .= .lastName "Johnson"
= .mary (john .clone .MaryObject)
mary .= .firstName "Mary"
print ((john .fullName) .+ " & " (mary .fullName))
user .method .fullName { "COOKIE MONSTER" }
print (john .fullName)

